{
  "name": "dev-toolbox",
  "version": "0.0.0",
  "description": "The useful development toolbox for Node.js packages.",
  "main": "src/index.js",
  "author": "Richard Szakacs, richardszkcs@gmail.com, www.richardszkcs.com",
  "homepage": "https://github.com/atjse/dev-toolbox#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atjse/dev-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/atjse/dev-toolbox/issues"
  },
  "license": "MIT",
  "keywords": [
    "dev",
    "development",
    "tool",
    "tools",
    "toolbox",
    "toolkit",
    "util",
    "utils",
    "helper",
    "helpers",
    "node",
    "nodejs",
    "package"
  ],
  "scripts": {
    "check": "standard --env mocha --verbose | snazzy",
    "fix": "standard --env mocha --fix",
    "test": "istanbul cover _mocha test --recursive",
    "cover": "npm run check && npm run test && istanbul check-coverage",
    "report": "npm run test -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "connect": "^3.6.6",
    "get-port": "^3.2.0",
    "pkg-dir": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  }
}
